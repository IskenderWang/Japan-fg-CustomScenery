    <animation>
        <type>noshadow</type>
        <object-name>WhiteLight6000</object-name>
    </animation>

    <animation>
        <type>alpha-test</type>
		<object-name>WhiteLight6000</object-name>
        <alpha-factor>0.01</alpha-factor>
    </animation>
	
    <animation>
        <type>select</type>
	<object-name>procedural-approach-static</object-name>
        <nopreview/>
        <condition>
	    <and>
                <property>/sim/rendering/shaders/skydome</property>

			<or>
				<greater-than>
					<property>/sim/time/sun-angle-rad</property>
					<value>1.4742</value>
				</greater-than>
				<less-than>
					<property>environment/visibility-m</property>
					<value>4500.00</value>
				</less-than>
			</or>
            
            </and>

        </condition>
    </animation>

<animation>
	<type>select</type>
	<object-name>approach-static-light</object-name>
	<nopreview/>
		<condition>
	        <not>
				<property>/sim/rendering/shaders/skydome</property>
            </not>
			<or>
				<greater-than>
					<property>/sim/time/sun-angle-rad</property>
					<value>1.4742</value>
				</greater-than>
				<less-than>
					<property>environment/visibility-m</property>
					<value>4500.00</value>
				</less-than>
			</or>

	</condition>
</animation>
	

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>15000</max-m>
	</animation>
	
