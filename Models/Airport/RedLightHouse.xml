<?xml version="1.0"?>

<PropertyList>

 <path>RedLightHouse.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>20000</max-m>
 </animation>

 <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>

 <animation>
  <type>noshadow</type>
  <object-name>Hull</object-name>
 </animation>


 <animation>
  <type>range</type>
  <object-name>Mast</object-name>
<!--  <condition>
   <less-than-equals>
    <property>/sim/time/sun-angle-rad</property>
    <value>0</value>
   </less-than-equals>
  </condition> -->
  <min-m>0</min-m>
  <max-m>5000</max-m>
 </animation>

 <animation>
  <type>range</type>
  <object-name>RedFlash.1</object-name>
  <min-m>0</min-m>
  <max-m>2500</max-m>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RedFlash.1</object-name>
  <property>/sim/time/elapsed-sec</property>
  <factor>90.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>4.7</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
 <animation>
  <type>flash</type>
  <object-name>RedFlash.1</object-name>
  <center>
   <x-m>0</x-m>
   <y-m>-0.35</y-m>
   <z-m>4.7</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0.1</z>
  </axis>
  <power>100</power>
 </animation>

